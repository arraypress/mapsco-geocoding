{
  "name": "arraypress/mapsco-geocoding",
  "version": "1.0.0",
  "description": "A PHP library for integrating with the Maps.co Geocoding API in WordPress, providing both forward and reverse geocoding capabilities with WordPress transient caching.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/mapsco-geocoding/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\MapsCo\\Geocoding\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/mapsco-geocoding/issues",
    "source": "https://github.com/arraypress/mapsco-geocoding"
  },
  "keywords": [
    "geocoding",
    "reverse-geocoding",
    "maps",
    "maps.co",
    "location",
    "coordinates",
    "address",
    "latitude",
    "longitude",
    "wordpress",
    "geolocation",
    "address-lookup",
    "coordinates-lookup",
    "location-data",
    "openstreetmap"
  ]
}